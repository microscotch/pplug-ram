<?xml version="1.0"?>
<wf-panel-pi>
	<plugin name="panel">
	<_short>Panel</_short>
	<group>
	<_short>RAM</_short>
	<option name="ram_show_percentage" type="bool">
		<_short>RAM Show Percentage</_short>
		<default>true</default>
	</option>
	<option name="ram_foreground" type="string">
		<_short>RAM Foreground Colour</_short>
		<default>dark gray</default>
	</option>
	<option name="ram_background" type="string">
		<_short>RAM Background Colour</_short>
		<default>light gray</default>
	</option>
	</group>
	</plugin>
</wf-panel-pi>
